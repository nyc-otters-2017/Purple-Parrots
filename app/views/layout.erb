<!DOCTYPE html>
<html lang="en">
<head>
  <link rel="stylesheet" href="/css/normalize.css">
  <link rel="stylesheet" href="/css/application.css">
  <link href="https://fonts.googleapis.com/css?family=Nunito+Sans:400,600" rel="stylesheet">

  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  <script src="/js/application.js"></script>
  <script type="text/javascript" src="https://cdn.jsdelivr.net/jdenticon/1.4.0/jdenticon.min.js"></script>
</head>
<body>
  <header>
    <div id="nav_bar">
      <ul id="nav_links">
      <% current_user %>>
      <% if @current_user %>
        <li><a href="/users/<% @current_user.id %>"> <%= @current_user.username %> </a></li>
        <li><a href="/users/logout"> Log Out </></li>
      <% else %>
        <li><a href="/users/new">Sign Up</a></li>
        <li><a href="/users/login">Log In</a></li>
      </ul>
      <% end %>
    </div>
    <div id="title_bar">
      <h1 id="title"><a href="/">KERNEL PANIC<a></h1>
      <ul id="buttons">
        <li><a href="/questions" class="button_links">Questions</a></li>
        <li><a href="/users" class="button_links">Users</a></li>
        <li><a href="/questions/new" class="button_links">Ask Question</a></li>
      </ul>
    </div>
  </header>
  <%= yield %>
</body>
</html>
